<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.covid19project">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".Covid19Project"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".DonateFundsActivity"></activity>
        <activity android:name=".EPassActivity" />
        <activity android:name=".CounsellingActivity" />
        <activity android:name=".VocationalTrainingCoursesActivity" />
        <activity android:name=".TestLabsActivity" />
        <activity android:name=".HospitalAdmissionsActivity" />
        <activity android:name=".VolunteersRegistrationActivity" />
        <activity android:name=".MedicalStoresActivity" />
        <activity android:name=".MyHealthStatusActivity" />
        <activity android:name=".HomeTreamentActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>